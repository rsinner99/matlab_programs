function integral_with_const(f, x1, x2, wert)
    syms x
    syms a
    format rational
    f = str2sym(f);
    stammfkt = int(f);
    F = matlabFunction(stammfkt);
    a = F(a,x2) - F(a,x1);
    
    fprintf('Stammfunktion F(x) = ')
    disp(stammfkt)
    fprintf('\n')
    fprintf('Wert des Integrals: ')
    disp(wert)
end